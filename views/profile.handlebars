<div class="navbar-fixed white black-text">
    <div class="row" id="brand-logo">
        <a href="/index" class="brand-logo">Screen Printer</a><br>
    </div>
    <div class="valign-wrapper">
         <ul id="nav-tabs" class="tabs center">
             <li class="tab col s6 active"><a href="/login" class="black-text">Login</a></li>
             <li class="tab col s6 active"><a href="/signup" class="black-text">Sign up</a></li>
             <li class="tab col s6 active"><a href="/index" class="black-text">Home</a></li>
             <li class="tab col s6 active"><a href="/about" class="black-text">About</a></li>
             {{#if user.username}}
                 <li class="tab col s3 active"><a href="/profile" class="black-text">Profile</a></li>
             {{/if}}
         </ul>
        </div>
    </div>


<div class="container">
    <div class="row col s12">
            <div class="card blue-grey darken-1" id="profileCard">
                <div class="card-content white-text">
                    <span class="card-title">User Information</span>
                    <p>
                        <strong>Username</strong>: {{user.username }}<br>
                        <strong>Password</strong>: {{ user.password }}<br>
                        <strong>First Name</strong>: {{ user.user_first_name }}<br>
                        <strong>Last Name</strong>: {{ user.user_last_name }}<br>
                        <strong>Phone Number</strong>: {{ user.user_number }}<br>
                        <strong>Email</strong>: {{ user.user_email }}<br>
                    </p>
                </div>
            </div>
        <div class="row col s12">
                <p><a href="/login">Login</a></p>
                <p>Go <a href="/">Home</a></p>
        </div>
    </div>
</div>

